---
const {images} = Astro.props;
---

{
    images.map((image:any) =>(
    <div class="hidden duration-700 ease-in-out" data-carousel-item>
         <img src={image} class="absolute block max-w-full h-auto -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2" alt={`captura de pantalla del proyecto`}>
     </div>
    ))
}
